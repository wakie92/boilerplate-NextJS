{
  "name": "boilerplate-front-end",
  "version": "1.0.0",
  "repository": "https://git-codecommit.us-west-2.amazonaws.com/v1/repos/boilerplate-front-end",
  "license": "MIT",
  "description": "Boilerplate project to use from when creating new projects.",
  "main": "pages/index.tsx",
  "author": "Guillaume Becker <guillaume@plutusds.com>",
  "private": true,
  "scripts": {
    "dev": "NODE_OPTIONS='--inspect' next dev",
    "build": "next build",
    "export": "rm -rf .next out && next build && next export",
    "start": "next start",
    "check-types": "tsc -p tsconfig.json --noEmit",
    "next-lint": "next lint",
    "lint": "./node_modules/.bin/eslint --fix ./",
    "cssLint": "stylelint './src/**/*.ts'",
    "tsLint": "eslint 'src/**/*.[jt]s?(x)'",
    "format": "prettier-eslint --write $PWD/'src/**/*.[jt]s?(x)' && eslint --fix .",
    "eslint-check": "eslint --print-config . | eslint-config-prettier-check",
    "deploy-serverless": "serverless",
    "prepare": "husky install",
    "pre-commit": "tsc -p tsconfig.json --noEmit && lint-staged",
    "preinstall": "npx only-allow pnpm"
  },
  "dependencies": {
    "@tanstack/react-query": "^4.29.7",
    "@tanstack/react-query-devtools": "^4.29.7",
    "@types/lodash-es": "^4.17.7",
    "@types/react-dom": "^18.2.0",
    "axios": "^1.4.0",
    "axios-retry": "^3.5.0",
    "bootstrap": "^5.2.3",
    "dayjs": "^1.11.7",
    "eventemitter3": "^5.0.1",
    "firebase": "^9.22.0",
    "html-react-parser": "^3.0.16",
    "jotai": "^2.1.0",
    "jotai-tanstack-query": "^0.7.0",
    "jwt-decode": "^3.1.2",
    "localforage": "^1.10.0",
    "lodash-es": "^4.17.21",
    "next": "^13.4.3",
    "next-seo": "^6.0.0",
    "nprogress": "^0.2.0",
    "raw-loader": "^4.0.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.8.0",
    "react-markdown": "^8.0.7",
    "reactstrap": "^9.1.10",
    "sanitize-html": "^2.10.0",
    "sass": "^1.62.1",
    "styled-components": "^6.0.0-rc.1",
    "validator": "^13.9.0"
  },
  "devDependencies": {
    "@next/eslint-plugin-next": "^13.4.3",
    "@types/lodash": "^4.14.194",
    "@types/node": "^20.2.3",
    "@types/react": "^18.2.0",
    "@types/react-redux-toastr": "^7.6.2",
    "@types/styled-components": "^5.1.26",
    "@typescript-eslint/eslint-plugin": "^5.59.6",
    "@typescript-eslint/parser": "^5.59.6",
    "eslint": "^8.41.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-lodash-fp": "^2.2.0-a1",
    "eslint-plugin-no-secrets": "^0.8.9",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-security": "^1.7.1",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.2",
    "prettier": "^2.8.8",
    "prettier-eslint": "^15.0.1",
    "prettier-eslint-cli": "^7.1.0",
    "snazzy": "^9.0.0",
    "stylelint": "^15.6.2",
    "stylelint-config-recommended": "^12.0.0",
    "stylelint-config-standard": "^33.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.10.0",
    "typescript": "^5.0.4"
  }
}
